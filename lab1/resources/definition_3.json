{
	"Comment": "Basic step function process to Validate IP and cellphone",
	"StartAt": "Validate",
	"States": {
		"Validate": {
			"Type": "Task",
			"Resource": "arn:aws:lambda:us-west-2:318126962944:function:validate",
			"Next": "Parallel"
		},
		"Parallel": {
			"Type": "Parallel",
			"Branches": [{
					"StartAt": "Tag",
					"States": {
						"Tag": {
							"Type": "Task",
							"Resource": "arn:aws:lambda:us-west-2:318126962944:function:tag",
							"End": true
						}
					}
				},{
					"StartAt": "Sentiment",
					"States": {
						"Sentiment": {
							"Type": "Task",
							"Resource": "arn:aws:lambda:us-west-2:318126962944:function:sentiment",
							"End": true
						}
					}
				}
			],
			"Next": "Publish"
		},
		"Publish": {
			"Type": "Task",
			"Resource": "arn:aws:lambda:us-west-2:318126962944:function:publish",
			"Next": "CreatePreSignedUrl"
		},
		"CreatePreSignedUrl": {
			"Type": "Task",
			"Resource": "arn:aws:lambda:us-west-2:318126962944:function:create_pre_signed_url",
			"Next": "SendText"
		},
		"SendText": {
			"Type": "Task",
			"Resource": "arn:aws:states:::sns:publish",
			"Parameters": {
				"Message.$": "$.message_str",
				"PhoneNumber.$": "$$.Execution.Input.cellphone_str"
			},
			"End": true
		}
	}
}